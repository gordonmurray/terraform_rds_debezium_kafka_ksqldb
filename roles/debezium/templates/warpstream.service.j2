[Unit]
Description=warpstream

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/
ExecStart=/home/ubuntu/.warpstream/warpstream agent -agentPoolName apn_default -bucketURL mem://{{ warpstream_s3_bucket }} -apiKey {{ warpstream_api_key }} -defaultVirtualClusterID {{ warpstream_cluster_id }} > /var/log/warpstream.log
Restart=always

[Install]
WantedBy=multi-user.target
